@model Sell__cleaning_services_e_commerce.Models.EmailLog

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/admin/_Layout.cshtml";
}
<div class="container my-4">
    <h1>Details</h1>
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">EmailLog</h4>
            <hr>
            <div class="row mb-3">
                <div class="col-sm-2 fw-bold">
                    @Html.DisplayNameFor(model => model.ToEmail)
                </div>
                <div class="col-sm-10">
                    @Html.DisplayFor(model => model.ToEmail)
                </div>
            </div>
            <div class="row mb-3">
                <div class="col-sm-2 fw-bold">
                    @Html.DisplayNameFor(model => model.Subject)
                </div>
                <div class="col-sm-10">
                    @Html.DisplayFor(model => model.Subject)
                </div>
            </div>
            <div class="row mb-3">
                <div class="col-sm-2 fw-bold">
                    @Html.DisplayNameFor(model => model.Message)
                </div>
                <div class="col-sm-10">
                    @Html.Raw(Model.Message)
                </div>
            </div>
            <div class="row mb-3">
                <div class="col-sm-2 fw-bold">
                    @Html.DisplayNameFor(model => model.SentDate)
                </div>
                <div class="col-sm-10">
                    @Html.DisplayFor(model => model.SentDate)
                </div>
            </div>
           
            <div class="row mb-3">
                <div class="col-sm-2 fw-bold">
                    @Html.DisplayNameFor(model => model.Sender)
                </div>
                <div class="col-sm-10">
                    @Html.DisplayFor(model => model.Sender.Id)
                </div>
            </div>
        </div>
    </div>
    <div class="mt-3">
        <a asp-action="Index" class="btn btn-primary">Back to List</a>
    </div>
</div>